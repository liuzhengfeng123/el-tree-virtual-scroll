{
  "name": "el-tree-virtual-scroll",
  "version": "1.0.0",
  "description": "A Virtual Scrolling Tree Implementation Using Element-UI's el-tree",
  "main": "dist/el-tree-virtual-scroll.umd.js",
  "module": "dist/el-tree-virtual-scroll.esm.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/el-tree-virtual-scroll.esm.js",
      "require": "./dist/el-tree-virtual-scroll.umd.js",
      "types": {
        "default": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      }
    }
  },
  "scripts": {
    "dev": "webpack serve --open --config build/webpack.dev.js",
    "build": "npm run clean && webpack --config build/webpack.prod.js && gulp",
    "clean": "rimraf dist",
    "lint": "eslint",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ]
  },
  "author": "liuzhengfeng <563596927@qq.com>",
  "keywords": [
    "vue",
    "el-tree-virtual-scroll",
    "virtual",
    "scroll",
    "el-tree",
    "ElementUI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liuzhengfeng123/el-tree-virtual-scroll.git"
  },
  "license": "ISC",
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.33.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "element-ui": "^2.15.14",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "~10.4.0",
    "fs-extra": "^11.3.1",
    "globals": "^16.3.0",
    "gulp": "^5.0.1",
    "gulp-sass": "^6.0.1",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "vue": "^2.7.16",
    "vue-loader": "^15.11.1",
    "vue-router": "^3.6.5",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
